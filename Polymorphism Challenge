class Vehicle:
    def move(self):
        pass  # Placeholder method

class Car(Vehicle):
    def move(self):
        return "🚗 Driving on the road!"

class Plane(Vehicle):
    def move(self):
        return "✈️ Flying in the sky!"

class Boat(Vehicle):
    def move(self):
        return "🚢 Sailing on water!"

# Creating Objects
vehicles = [Car(), Plane(), Boat()]

# Using Polymorphism
for v in vehicles:
    print(v.move())

# Output:
# 🚗 Driving on the road!
# ✈️ Flying in the sky!
# 🚢 Sailing on water!
