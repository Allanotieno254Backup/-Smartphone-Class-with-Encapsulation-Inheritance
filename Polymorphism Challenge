class Vehicle:
    def move(self):
        pass  # Placeholder method

class Car(Vehicle):
    def move(self):
        return "ğŸš— Driving on the road!"

class Plane(Vehicle):
    def move(self):
        return "âœˆï¸ Flying in the sky!"

class Boat(Vehicle):
    def move(self):
        return "ğŸš¢ Sailing on water!"

# Creating Objects
vehicles = [Car(), Plane(), Boat()]

# Using Polymorphism
for v in vehicles:
    print(v.move())

